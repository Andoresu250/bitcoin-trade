<p id="notice"><%= notice %></p>

<h1>Charge Points</h1>

<table>
  <thead>
    <tr>
      <th>Owner</th>
      <th>Account type</th>
      <th>Number</th>
      <th>Owner identification</th>
      <th>Iban</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @charge_points.each do |charge_point| %>
      <tr>
        <td><%= charge_point.owner %></td>
        <td><%= charge_point.account_type %></td>
        <td><%= charge_point.number %></td>
        <td><%= charge_point.owner_identification %></td>
        <td><%= charge_point.iban %></td>
        <td><%= link_to 'Show', charge_point %></td>
        <td><%= link_to 'Edit', edit_charge_point_path(charge_point) %></td>
        <td><%= link_to 'Destroy', charge_point, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Charge Point', new_charge_point_path %>
